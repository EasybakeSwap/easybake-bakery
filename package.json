{
  "name": "easybake-bakery",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate": "truffle migrate --reset",
    "migrate:rinkeby": "truffle migrate --reset --network rinkeby",
    "compile": "truffle compile",
    "compile:rinkeby": "truffle compile --network rinkeby",
    "verify:oven": "truffle run verify OvenToken --network rinkeby --license SPDX-License-Identifier",
    "verify:sugar": "truffle run verify SugarBar --network rinkeby --license SPDX-License-Identifier",
    "verify:master": "truffle run verify MasterChef --network rinkeby --license SPDX-License-Identifier",
    "flatten:oven": "./node_modules/.bin/truffle-flattener contracts/OvenToken.sol > flats/OvenToken_flat.sol",
    "test": "npx buidler test",
    "coverage": "npx buidler coverage",
    "flatten:oven": "truffle-flattener ./contracts/OvenToken.sol > ./flats/OvenToken.sol",
    "flatten:bar": "truffle-flattener ./contracts/SugarBar.sol > ./flats/SugarBar_flat.sol",
    "flatten:chef": "truffle-flattener ./contracts/MasterChef.sol > ./flats/SugarBar_flat.sol"
  },
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.6",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "easybake-swap-core": "^0.2.0",
    "easybake-swap-lib": "^0.1.0",
    "easybake-swap-periphery": "^1.0.1",
    "truffle": "^5.3.0",
    "truffle-flattener": "^1.5.0",
    "truffle-hdwallet-provider-privkey": "^0.3.0"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.14",
    "solidity-coverage": "^0.7.13",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.4.0",
    "web3": "^1.3.0"
  }
}
