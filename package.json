{
  "name": "easybake-bakery",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate": "truffle migrate --reset",
    "migrate:rinkeby": "truffle migrate --reset --network rinkeby",
    "compile": "truffle compile",
    "compile:rinkeby": "truffle compile --network rinkeby",
    "verify:bar": "truffle run verify SugarBar --network rinkeby --license SPDX-License-Identifier",
    "verify:oven": "truffle run verify OvenToken --network rinkeby --license SPDX-License-Identifier",
    "verify:sugar": "truffle run verify SugarBar --network rinkeby --license SPDX-License-Identifier",
    "verify:chef": "truffle run verify MasterChef --network rinkeby --license SPDX-License-Identifier",
    "verify:vault": "truffle run verify OvenVault --network rinkeby --license SPDX-License-Identifier",
    "flat:bar": "/node_modules/.bin/truffle-flattener contracts/SugarBar.sol > flats/SugarBar_flat.sol",
    "flat:oven": "./node_modules/.bin/truffle-flattener contracts/OvenToken.sol > flats/OvenToken_flat.sol",
    "flat:chef": "./node_modules/.bin/truffle-flattener contracts/MasterChef.sol > flats/MasterChef_flat.sol",
    "flat:vault": "./node_modules/.bin/truffle-flattener contracts/OvenVault.sol > flats/OvenVault_flat.sol",
    "test": "npx buidler test",
    "coverage": "npx buidler coverage"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "truffle": "^5.3.4",
    "truffle-flattener": "^1.5.0",
    "truffle-hdwallet-provider-privkey": "^0.3.0"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/buidler-ethers": "^2.0.2",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-waffle": "^2.1.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.14",
    "solidity-coverage": "^0.7.16",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.7",
    "web3": "^1.3.5"
  }
}
